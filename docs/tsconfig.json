{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "strict": false,
    "exactOptionalPropertyTypes": false,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
